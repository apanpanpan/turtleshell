<div class="filters">
  <div class="dropdown">
    <button class="dropbtn">Building</button>
    <div class="dropdown-content">
      <a href="javascript:param('building', 'l', {{#if vals.l}}3{{else}}2{{/if}});">Landmark</a>
      <a href="javascript:param('building', 'c', {{#if vals.c}}3{{else}}2{{/if}});">South Campus Commons</a>
      <a href="javascript:param('building', 't', {{#if vals.t}}3{{else}}2{{/if}});">Terrapin Row</a>
      <a href="javascript:param('building', 'y', {{#if vals.y}}3{{else}}2{{/if}});">Varsity</a>
      <a href="javascript:param('building', 'w', {{#if vals.w}}3{{else}}2{{/if}});">View</a>
      <a href="javascript:param('building', 'o', {{#if vals.o}}3{{else}}2{{/if}});">Off Campus Housing</a>
    </div>
  </div>
  &nbsp; &nbsp;
  <div class="dropdown">
    <button class="dropbtn">Gender</button>
    <div class="dropdown-content">
      <a href="javascript:param('gender', 'm', {{#if vals.m}}4{{else}}1{{/if}});">Male</a>
      <a href="javascript:param('gender', 'f', {{#if vals.fem}}4{{else}}1{{/if}});">Female</a>
      <a href="javascript:param('gender', 'f', 4);">Mixed</a>
    </div>
  </div>
  &nbsp; &nbsp;
  <div class="dropdown">
    <button class="dropbtn">Semester</button>
    <div class="dropdown-content">
      <a href="javascript:param('semester', 'f', {{#if vals.f}}3{{else}}2{{/if}});">Fall</a>
      <a href="javascript:param('semester', 'w', {{#if vals.wint}}3{{else}}2{{/if}});">Winter</a>
      <a href="javascript:param('semester', 'p', {{#if vals.p}}3{{else}}2{{/if}});">Spring</a>
      <a href="javascript:param('semester', 's', {{#if vals.s}}3{{else}}2{{/if}});">Summer</a>
    </div>
  </div>
  &nbsp; &nbsp;
  <div class="dropdown">
    <button class="dropbtn">Group Size</button>
    <div class="dropdown-content">
      <a href="javascript:param('group', '1', {{#if vals.one}}4{{else}}1{{/if}})">1</a>
      <a href="javascript:param('group', '2', {{#if vals.two}}4{{else}}1{{/if}})">2</a>
      <a href="javascript:param('group', '3', {{#if vals.three}}4{{else}}1{{/if}})">3</a>
      <a href="javascript:param('group', '4', {{#if vals.four}}4{{else}}1{{/if}})">4+</a>
    </div>
  </div>
</div>
<article>
<div class="main">
    <div class="split left">
      <img src={{builds.roomUrl}} width = 350px height = 230px>
      <h2>{{builds.building}}</h2>
      <div class="Roominfo">
        {{{builds.roomInfo}}}
      </div>
      <p>{{builds.roomBio}}</p>
    </div>
    <div class="split right">
      <div class="lenderphoto">
        <img src={{builds.url}} width = 150px height = 150px>
      </div>
      <p>{{builds.bio}}</p>
    </div>

    <div class="happysad">
      <input type="image" src="https://i.imgur.com/P8rrcaW.png" height=70px width=70px/>
    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
      <input type="image" src="https://i.imgur.com/7Xp7K8l.png" height=70px width=70px/>
    </div>
</div>
</article>
</html>
